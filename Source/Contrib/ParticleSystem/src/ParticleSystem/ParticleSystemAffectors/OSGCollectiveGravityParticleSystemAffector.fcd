    <Field
        name="ParticleMassSource"
        type="UInt32"
        category="data"
        cardinality="single"
        visibility="external"
        access="public"
        defaultValue="CollectiveGravityParticleSystemAffector::MASS_STATIC"
    >
    </Field>
    <Field
        name="MinDistance"
        type="Real32"
        category="data"
        cardinality="single"
        visibility="external"
        access="public"
        defaultValue="-1.0"
    >
    </Field>
    <Field
        name="MaxDistance"
        type="Real32"
        category="data"
        cardinality="single"
        visibility="external"
        access="public"
        defaultValue="-1.0"
    >
    </Field>
</FieldContainer>
